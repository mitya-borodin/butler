{
  "name": "objectd",
  "private": true,
  "version": "1.0.0",
  "license": "unlicensed",
  "scripts": {
    "start": "nodemon --config nodemon.json --inspect=0.0.0.0:9229 src/index.ts | pino-pretty -c",
    "prisma:generate": "prisma generate --schema ./src/infrastructure/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --name init --schema ./src/infrastructure/prisma/schema.prisma",
    "prisma:studio": "cd ./src/infrastructure/prisma && prisma studio",
    "test": "jest --no-cache",
    "build": "del dist && tsc --project ./tsconfig.build.json",
    "ncu:outdated": "npm-check-updates",
    "ncu:upgrade": "npm-check-updates -u --target minor",
    "postinstall": "npm run prisma:generate"
  },
  "engines": {
    "node": ">=14.18.2"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "bson": "^4.6.1",
    "co-body": "^6.1.0",
    "dotenv": "^14.2.0",
    "eventemitter3": "^4.0.7",
    "express": "^4.17.3",
    "file-type": "^16.5.3",
    "formidable": "^2.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-pino-logger": "^4.0.0",
    "koa-router": "^10.1.1",
    "mongodb": "^4.2.1",
    "ot-json1": "^1.0.2",
    "pino": "^7.8.0",
    "sharedb": "^2.2.1",
    "sharedb-milestone-mongo": "^0.7.0",
    "sharedb-mongo": "^1.0.0-beta.21",
    "type-is": "^1.6.18",
    "uuid": "^8.3.2",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/bson": "^4.2.0",
    "@types/co-body": "^6.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/file-type": "^10.9.1",
    "@types/formidable": "^2.0.4",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-pino-logger": "^3.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/mocha": "^9.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.26",
    "@types/sharedb": "^2.2.0",
    "@types/type-is": "^1.6.3",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
    "del-cli": "^4.0.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.2",
    "pino-pretty": "^7.5.1",
    "prisma": "^3.9.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
